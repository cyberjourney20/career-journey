create_table("skills") {
  t.Column("id", "integer", {primary: true})
  t.Column("domain", "string", {"default": ""})
  t.Column("skill_name", "string", {"default": ""})
  t.Column("url", "string", {"default": ""})
  t.Column("description", "string", {"default": ""})
  t.Column("demand", "jsonb" , {"default": "{}"})
}